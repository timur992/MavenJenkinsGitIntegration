<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Cross Browser Testing">
	<test thread-count="5" name="Firefox Browser Test">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="integration.NegativeParameterisedTest" />
		</classes>
	</test> <!--
	Test -->

	<test thread-count="5" name="Edge Browser Test">
		<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="integration.NegativeParameterisedTest" />
		</classes>
	</test> <!--
	Test -->

	<test thread-count="5" name="Chrome Browser Test">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="integration.NegativeParameterisedTest" />
		</classes>
	</test> <!--
	Test -->
</suite> <!--
Suite -->
